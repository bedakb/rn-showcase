    <parameter id="STREETNUMBER" key="STREETNUMBER"></parameter>
    <parameter id="ZIP" key="ZIP"></parameter>
    
    
    <!--define the global page parameter, the key is the index,  -->
    <pageParameter>
      <parameter id="1" key="Key1"></parameter>
